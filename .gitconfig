[user]
	name = Pranav Kulkarni
	email = pranavcode@gmail.com
[core]
	editor = emacs -nw
	autocrlf = false
	fileMode = false
	repositoryFormatVersion = 0
	symlinks = true
	logAllRefUpdates = true
[color]
	ui = true
[rebase]
	autosquash = true
[credential]
	helper = cache --timeout=86400
[push]
	default = simple
[branch "master"]
	remote = origin
	merge = refs/heads/master
